{
  "rules": {
    "strict": [
      2,
      "global"
    ],
    "quotes": [
      2,
      "single"
    ],
    "indent": [
      0,
      4
    ],
    "consistent-return": 0,
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "space-before-function-paren": [
      2,
      "never"
    ],
    "keyword-spacing": 2,
    "space-in-parens": [
      2,
      "never"
    ],
    "quote-props": [
      2,
      "as-needed"
    ],
    "no-multiple-empty-lines": [
      2,
      {
        "max": 2
      }
    ],
    "brace-style": [
      2,
      "1tbs",
      { "allowSingleLine": true }
    ],
    "curly": [
      2,
      "all"
    ],
    "key-spacing": [
      0,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "space-infix-ops": 2,
    "no-eval": 2,
    "no-with": 2,
    "eol-last": 0,
    "comma-dangle": [
      0,
      "never"
    ],
    "no-cond-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": [
      2,
      "functions"
    ],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "dot-notation": [
      2,
      {
        "allowKeywords": true
      }
    ],
    "eqeqeq": [2, "allow-null"],
    "no-alert": 2,
    "no-caller": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 0,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-unused-expressions": 2,
    "yoda": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 0,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "none"
      }
    ],
    "camelcase": 2,
    "comma-spacing": 2,
    "new-cap": 2,
    "new-parens": 0,
    "no-array-constructor": 2,
    "no-mixed-spaces-and-tabs": [
      2,
      false
    ],
    "no-new-object": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "semi": 2
  },
  "env": {
    "node": true,
    "mocha": true
  }
}
